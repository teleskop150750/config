  "author": "Валентин Степанов <teleskop150750@gmail.com> (https://github.com/teleskop150750/)",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "eslint": "eslint \"test/**/*.{vue,ts,js}\"",
    "eslint:fix": "eslint \"test/**/*.{vue,ts,js}\" --fix",
    "stylelint": "stylelint \"test/**/*.{vue,css}\"",
    "stylelint:fix": "stylelint \"test/**/*.{vue,css}\" --fix",
    "prettier": "prettier --check --ignore-unknown \"test/**/*\"",
    "prettier:fix": "prettier \"test/**/*\" --write",
    "release": "lerna publish --conventional-commits"
  },
  "devDependencies": {
    "@teleskop150750/eslint-config-vue": "*",
    "@teleskop150750/prettier-config": "*",
    "@teleskop150750/stylelint-config-css": "*",
    "@teleskop150750/stylelint-config-vue": "*",
    "@teleskop150750/tsconfig": "*",
    "eslint": "^8.8.0",
    "lerna": "^4.0.0",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.3.0",
    "typescript": "^4.5.5"
  }
}
